+@@ -24,15 +25,15 @@
+ #else /* HAVE_MEMSET_S */
+ 
+ /*
+- * Indirect bzero through a volatile pointer to hopefully avoid
++ * Indirect memset through a volatile pointer to hopefully avoid
+  * dead-store optimisation eliminating the call.
+  */
+-static void (* volatile ssh_bzero)(void *, size_t) = bzero;
++static void *(* volatile ssh_memset)(void *s, int c, size_t n) = memset;
+ 
+ void
+ explicit_bzero(void *p, size_t n)
+ {
+-	ssh_bzero(p, n);
++	(void)ssh_memset(p, 0, n);
+ }
+ 
+ #endif /* HAVE_MEMSET_S */
+diff -Nurb openssh-6.8p1/openbsd-compat/explicit_bzero.c.orig openssh-6.8p1.n//openbsd-compat/explicit_bzero.c.orig
+--- openssh-6.8p1/openbsd-compat/explicit_bzero.c.orig	1970-01-01 01:00:00.000000000 +0100
++++ openssh-6.8p1.n//openbsd-compat/explicit_bzero.c.orig	2015-03-17 06:49:20.000000000 +0100
+@@ -0,0 +1,40 @@
++/* OPENBSD ORIGINAL: lib/libc/string/explicit_bzero.c */
++/*	$OpenBSD: explicit_bzero.c,v 1.1 2014/01/22 21:06:45 tedu Exp $ */
++/*
++ * Public domain.
++ * Written by Ted Unangst
++ */
++
++#include "includes.h"
++
++/*
++ * explicit_bzero - don't let the compiler optimize away bzero
++ */
++
++#ifndef HAVE_EXPLICIT_BZERO
++
++#ifdef HAVE_MEMSET_S
++
++void
++explicit_bzero(void *p, size_t n)
++{
++	(void)memset_s(p, n, 0, n);
++}
++
++#else /* HAVE_MEMSET_S */
++
++/*
++ * Indirect bzero through a volatile pointer to hopefully avoid
++ * dead-store optimisation eliminating the call.
++ */
++static void (* volatile ssh_bzero)(void *, size_t) = bzero;
++
++void
++explicit_bzero(void *p, size_t n)
++{
++	ssh_bzero(p, n);
++}
++
++#endif /* HAVE_MEMSET_S */
++
++#endif /* HAVE_EXPLICIT_BZERO */
